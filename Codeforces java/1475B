import  java.util.*;

public class Main {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int n,result=0;
        short t;
        t=input.nextShort();

        while(t-->0){
            int k=3;
            n=input.nextInt();

            while(n%k>0){
                k+=(k+1);
            }
            System.out.println(n/k);
        }
    }
}
