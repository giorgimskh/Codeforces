#include <iostream>
using namespace std;

int main(){
    string s;
    cin >> s;
    
    for(short i=0;i<s.size();i+=3){       
        if(s[i]=='W' && s[i+1]=='U' && s[i+2]=='B') continue;     
            else{
                while(i<s.size()){
                    cout << s[i]; ++i;
                    if(s[i]=='W' && s[i+1]=='U' && s[i+2]=='B') break;
                }
                cout << " ";
            }
    }
}
