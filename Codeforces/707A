#include <iostream>
using namespace std;

int main(){
    short n,m,ans=0;
    char c;
    cin >> n >> m;
    
    for(short i=0;i<n;++i){
        for(short j=0;j<m;++j){
            cin >> c;
            if(c=='C' || c=='M' || c=='Y') ++ans;
        }
    }
    
    cout <<(ans>0 ?  "#Color":"#Black&White");
}
