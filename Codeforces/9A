#include <iostream>
using namespace std;

int main(){
    short point1,point2;
    cin >> point1 >> point2;   
    if(point1<point2) swap(point1,point2);
    
    if(point1==1) cout << 1 << "/"<<1;
    else if((6-point1+1)%2==0) cout << (6-point1+1)/2 << "/" <<3;
    else if((6-point1+1)%3==0) cout <<(6-point1+1)/3 << "/" << 2;
        else cout <<6-point1+1 << "/" << 6;
}
