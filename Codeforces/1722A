#include <iostream>
#include <algorithm>
using namespace std;

int main(){
    short t,k;
    string s;
    cin >> t;
    
    while(t--){
        cin >> k >>s;
             
        if(k!=5) cout << "NO" << endl;
            else {
                sort(s.begin(),s.end());
                if(s=="Timru") cout <<"YES" <<endl;
                    else cout << "NO" << endl;
            }
    }
}
