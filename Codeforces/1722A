#include <iostream>
using namespace std;

bool check(string s,short k){
    for(int i=0;i<k;++i){
      if(s[i]=='T' || s[i]=='i' || s[i]=='m' || s[i]=='u' || s[i]=='r') continue;
          else return false;
       }
    return true;
}

int main(){
    short t,k;
    string s;
    cin >> t;
    
    while(t--){
        cin >> k >>s;
        
        
        if(k!=5) cout << "NO" << endl;
           else cout <<(check(s,k) ? "YES":"NO") <<endl;
    }
}
