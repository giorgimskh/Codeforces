#include <iostream>
using namespace std;

int main(){
    short n,m;
    cin >> n >> m;
    
    while(n<=m){
        if(n%2) n+=2;
            else ++n;
        
        if((n%3==0 && n!=3) || (n%7==0 && n!=7) || n%2==0 || (n%5==0 && n!=5)) continue;
            else {cout <<(n==m ? "YES":"NO"); return 0;}
    }
    cout << "NO";
}
   
