#include <iostream>
using namespace std;

int main(){
    short t;
    string s;
    cin >> t;
    
    while(t--){
        string s1;
        cin >> s;
        
        for(char c:s){
            c=toupper(c);
            s1+=c;
        }
       if(s1=="YES") cout << "YES" << endl;
         else cout << "NO" << endl;
    }
}
